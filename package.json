{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "verify": "run format",
    "versions": "echo '[node]' && node --version && echo '\n[yarn]' && yarn --version && echo '\n[prettier]' && prettier --version"
  },
  "packageManager": "yarn@4.2.2",
  "devDependencies": {
    "prettier": "^3.2.5"
  },
  "prettier": "@pandell/prettier-config"
}
