{
  "name": "@pandell/eslint-config",
  "version": "8.0.2",
  "description": "Pandell Shared Config",
  "keywords": [
    "eslint",
    "eslintconfig",
    "pandell",
    "pli",
    "stylelint",
    "tsconfig"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pandell/web-pli"
  },
  "license": "MIT",
  "author": {
    "name": "Pandell Technology Corporation",
    "url": "https://github.com/pandell"
  },
  "main": "./index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "browserslist": "yarn supported:prod",
    "browserslist:dev": "npx browserslist --env=development",
    "browserslist:prod": "npx browserslist --env=production",
    "browserslist:test": "npx browserslist --env=test",
    "eslint:check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "dependencies": {
    "@typescript-eslint/parser": "~5.62.0",
    "eslint-config-prettier": "^8.10.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">= 5",
    "eslint": ">= 8",
    "eslint-plugin-css-modules": ">= 2",
    "eslint-plugin-import": ">= 2",
    "eslint-plugin-jsdoc": ">= 44",
    "eslint-plugin-react": ">= 7",
    "eslint-plugin-react-hooks": ">= 4",
    "eslint-plugin-simple-import-sort": ">= 10"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-css-modules": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
